{
  "manifest_version": 3,
  "name": "Mood Music Player",
  "version": "1.0",
  
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
